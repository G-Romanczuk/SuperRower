@model SuperRowerDB.RowerPrice

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Detale Roweru</h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Details">
